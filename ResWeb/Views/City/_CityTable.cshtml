
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>序号</th>
            <th>城市编码</th>
            <th>城市名称</th>
        </tr>
    </thead>
    <tbody>
        @{
            int id = 1;
            if (ViewBag.cityList != null)
            {
                foreach (var item in ViewBag.cityList)
                {
                    <tr>
                        <td id="key" style="display:none">@item.ID</td>
                        <td>@id</td>
                        <td>@item.CityCode</td>
                        <td>@item.CityName</td>
                    </tr>
                    id++;
                }
            }
        }
    </tbody>
</table>
