
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>序号</th>
            <th>资源编码</th>
            <th>资源名称</th>
            <th>园区</th>
            <th>建筑</th>
            <th>楼层</th> 
        </tr>
    </thead>
    <tbody>
        @{
            int id = 1;
            if (ViewBag.roomList != null)
            {
                foreach (var item in ViewBag.roomList)
                {
                    <tr>
                        <td class="pkey" style="display:none">@item.RID</td>
                        <td>@id</td>
                        <td>@item.RID</td>
                        <td>@item.RNAME</td>
                        <td>@item.PNAME</td>
                        <td>@item.BNAME</td>
                        <td>@item.FNAME</td>
                    </tr>
                    id++;
                }
            }
            else
            {
                <tr><td style="text-align:center" colspan="7">数据加载异常</td></tr>
            }
        }
    </tbody>
</table>





