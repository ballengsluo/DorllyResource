
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>序号</th>            
            <th>城市</th>
            <th>区域</th>
            <th>编码</th>
            <th>名称</th>
            <th>地址</th>
            <th>经度(X)</th>
            <th>纬度(Y)</th>
        </tr>
    </thead>
    <tbody>
        @{
            int id = 1;
            if (Model != null)
            {
                foreach (var item in Model)
                {
                    <tr>
                        <td id="key" style="display:none">@item.ID</td>
                        <td>@id</td>                        
                        <td>@item.CityName</td>
                        <td>@item.RegionName</td>
                        <td>@item.Code</td>
                        <td>@item.ParkShortName</td>
                        <td>@item.Addr</td>
                        <td>@item.GisX</td>
                        <td>@item.GisY</td>
                    </tr>
                    id++;
                }
            }
        }
    </tbody>
</table>


