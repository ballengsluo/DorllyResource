
<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>序号</th>
            <th>建设期编码</th>
            <th>园区名称</th>
            <th>建设期名称</th>
        </tr>
    </thead>
    <tbody>
        @{
            int id = 1;
            if (ViewBag.stageList != null)
            {
                foreach (var item in ViewBag.stageList)
                {
                    <tr>
                        <td class="pkey" style="display:none">@item.stage.ID</td>
                        <td>@id</td>
                        <td>@item.stage.Code</td>
                        <td>@item.park.Name</td>
                        <td>@item.stage.Name</td>
                    </tr>
                    id++;
                }
            }
            else
            {
                <tr><td style="text-align:center" colspan="4">数据加载异常</td></tr>
            }
        }
    </tbody>
</table>



