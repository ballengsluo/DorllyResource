
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>添加工位</title>
    @Styles.Render("~/common/css")
    @Styles.Render("~/ce/css")
</head>
<body>
    <div class="container">
        <div style="max-width:900px;margin:0 auto;">
            <h3 style="margin:30px 0px;text-align:center">添加工位</h3>
            @using (Ajax.BeginForm("Create", null, new AjaxOptions
            {
                HttpMethod = "POST",
                OnSuccess = "commitResult",
                OnFailure = "commitResult"
            }, new { @class = "form-horizontal" }))
            {
                <div class="form-group">
                    <label class="control-label col-md-2">城市：</label>
                    <div class="col-md-4">
                        <select class="selectpicker form-control pull-righ" name="City" id="City" title="请选择"></select>
                    </div>
                    <label class="control-label col-md-2">行政区：</label>
                    <div class="col-md-4">
                        <select class="selectpicker form-control pull-righ" name="Region" id="Region" title="请选择"></select>
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-2">园区：</label>
                    <div class="col-md-4">
                        <select class="selectpicker form-control pull-righ" name="ParkCode" id="Park" title="请选择"></select>
                    </div>
                    <label class="control-label col-md-2">建设期：</label>
                    <div class="col-md-4">
                        <select class="selectpicker form-control pull-righ" name="Stage" id="Stage" title="请选择"></select>
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-2">建筑：</label>
                    <div class="col-md-4">
                        <select class="selectpicker form-control pull-righ" name="Building" id="Building" title="请选择"></select>
                    </div>
                    <label class="control-label col-md-2">楼层：</label>
                    <div class="col-md-4">
                        <select class="selectpicker form-control pull-righ" name="Floor" id="Floor" title="请选择"></select>
                    </div>
                </div>              

                <div class="form-group">
                    <label class="control-label col-md-2">工位编码：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" name="CubicleCode" id="CubicleCode">
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-2">工位人数：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" name="CubicleSeat" id="CubicleSeat">
                    </div>
                    <label class="control-label col-md-2">工位单价：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" name="CubicleUnitPrice" id="CubicleUnitPrice">
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-2">可租个数：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" name="RentRangeSeat" id="RentRangeSeat">
                    </div>
                    <label class="control-label col-md-2">起租个数：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" name="RentBeginSeat" id="RentBeginSeat">
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-2">工位分组：</label>
                    <div class="col-md-4">
                        <select class="selectpicker form-control pull-righ" name="GroupCode" id="Group" title="请选择"></select>
                    </div>
                    <label class="control-label col-md-2">位置：</label>
                    <div class="col-md-4">
                        @*<textarea class="form-control" name="CubicleAddr" id="CubicleAddr"></textarea>*@
                        <input type="text" class="form-control" name="CubicleAddr" id="CubicleAddr" onmouseover="this.title=this.value">
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-2">工位图片：</label>
                    <div class="col-md-10">

                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-2">工位详情：</label>
                    <div class="col-md-10">
                        <script id="Content" name="CubicleContent" type="text/plain"></script>
                    </div>
                </div>

                <div class="form-group">
                    <label class="control-label col-md-2">最后修改人：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" name="UpdateUser" id="UpdateUser">
                    </div>
                    <label class="control-label col-md-2">最后修改时间：</label>
                    <div class="col-md-4">
                        <input type="text" class="form-control" name="UpdateTime" id="UpdateTime">
                    </div>
                </div>

                <p style="text-align:center;margin:40px 0px;">
                    <input type="submit" class="btn btn-primary" value="提交">
                    <input type="button" class="btn btn-danger" value="关闭" onclick="javascript: parent.layer.closeAll();" style="margin-left:10px;">
                </p>
            }
        </div>
    </div>
    @Scripts.Render("~/common/js")
    @Scripts.Render("~/ce/js")
</body>
</html>
