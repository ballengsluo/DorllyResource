@if (ViewData["dataList"] != null)
{
    @Html.DropDownList("dataList", ViewData["dataList"] as SelectList, new {@id=ViewBag.nodeId, @class = "selectpicker form-control pull-righ", @title = "请选择..." })
}
else
{
    <select class="selectpicker form-control pull-righ" id="@ViewBag.nodeId" title="暂无数据" disabled></select>
}
