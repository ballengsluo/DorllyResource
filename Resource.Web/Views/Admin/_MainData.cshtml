@helper rm()
{
    if (ViewBag.rm != null && ViewBag.rm.Count > 0)
    {
        <table class="table">
            <tr>
                <td>楼栋</td>
                <td>空置面积</td>
                <td>空置套数</td>
            </tr>
            @foreach (var item in ViewBag.rm)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.Area</td>
                    <td>@item.Number</td>
                </tr>
            }
            <tr>
                <td>汇总</td>
                <td>@ViewBag.AreaCount</td>
                <td>@ViewBag.NumberCount</td>
            </tr>
        </table>
    }
    else
    {
        <div>暂无数据</div>
    }

}
@helper cb()
{
    if (ViewBag.cb != null && ViewBag.cb.Count > 0)
    {
        <table class="table">
            <tr>
                <td>空置个数</td>
                <td>出租率</td>
            </tr>
            @foreach (var item in ViewBag.cb)
            {
                <tr>
                    <td>@item.Number</td>
                    <td>@item.Rate</td>
                </tr>
            }
        </table>
    }

}
