@if (ViewBag.model == 1)
{
    @Html.DropDownList("ParentID", ViewData["dataList"] as SelectList,
    new
    {
        @class = "selectpicker form-control pull-righ",
        @id = "room",
        @required = "required",
        @data_live_search="true",
        @title = "请选择..."
    })
}
else
{
    @Html.DropDownList("ParentID",
        ViewData["dataList"] as SelectList,
        "全部",
        new
        {
            @class = "selectpicker form-control pull-righ",
            @id = "room"
        })
}
