@if (ViewBag.model == 1)
{
    @Html.DropDownList("RegionID", ViewData["dataList"] as SelectList,
    new
    {
        @class = "selectpicker form-control pull-righ",
        @id = "region",
        @required = "required",
        @title = "请选择..."
    })
}
else
{
    @Html.DropDownList("RegionID",
        ViewData["dataList"] as SelectList,
        "全部",
        new
        {
            @class = "selectpicker form-control pull-righ",
            @id = "region"
        })
}




