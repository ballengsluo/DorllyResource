@if (ViewBag.model == 1)
{
    @Html.DropDownList("FloorID", ViewData["dataList"] as SelectList,
    new
    {
        @class = "selectpicker form-control pull-righ",
        @id = "floor",
        @required = "required",
        @title = "请选择..."
    })
}
else
{
    @Html.DropDownList("FloorID",
        ViewData["dataList"] as SelectList,
        "全部",
        new
        {
            @class = "selectpicker form-control pull-righ",
            @id = "floor"
        })
}